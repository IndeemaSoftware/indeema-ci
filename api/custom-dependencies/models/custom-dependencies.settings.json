{
  "connection": "default",
  "collectionName": "custom_dependencies",
  "info": {
    "name": "Custom Dependencies"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "install_script": {
      "type": "richtext"
    },
    "module": {
      "type": "string",
      "configurable": false
    },
    "servers": {
      "collection": "server",
      "via": "custom_dependencies",
      "dominant": true
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "custom_dependencies",
      "dominant": true
    }
  }
}

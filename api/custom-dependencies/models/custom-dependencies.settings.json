{
  "connection": "default",
  "collectionName": "custom_dependencies",
  "info": {
    "name": "Custom Dependencies"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "label": {
      "type": "string"
    },
    "install_script": {
      "type": "richtext"
    },
    "servers": {
      "via": "custom_dependencies",
      "collection": "server",
      "dominant": true
    }
  }
}

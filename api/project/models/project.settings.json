{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "project_name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "apps": {
      "collection": "app",
      "via": "project"
    },
    "desc": {
      "type": "text"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "projects",
      "dominant": true
    }
  }
}

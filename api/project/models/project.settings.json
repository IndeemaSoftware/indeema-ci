{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "Project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "project_name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "apps": {
      "via": "project",
      "collection": "app"
    },
    "desc": {
      "type": "text"
    },
    "environments": {
      "type": "json"
    }
  }
}

{
  "connection": "default",
  "collectionName": "services",
  "info": {
    "name": "Service"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "service_name": {
      "type": "string"
    },
    "variables": {
      "type": "json"
    },
    "doc": {
      "type": "json"
    },
    "setup_script": {
      "type": "richtext"
    },
    "cleanup_script": {
      "type": "richtext"
    },
    "module": {
      "type": "string",
      "configurable": false
    },
    "apps": {
      "collection": "app",
      "via": "service"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "services",
      "dominant": true
    }
  }
}

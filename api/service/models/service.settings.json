        "project": "",
        "environment": "",
        "desc": "",
        "server": "",
        "automatic_cert": "",
        "ci_template": "",
        "ci_script": "",
        "environments": ""
      }
    },
    "setup_script": {
      "type": "richtext"
    },
    "cleanup_script": {
      "type": "richtext"
    },
    "apps": {
      "via": "service",
      "collection": "app"
    }
  }

{
  "connection": "default",
  "collectionName": "servers",
  "info": {
    "name": "Server"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Server_name": {
      "type": "string"
    },
    "user": {
      "via": "servers",
      "model": "user"
    },
    "apps": {
      "collection": "app",
      "via": "server"
    }
  }
}

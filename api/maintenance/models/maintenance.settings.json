{
  "connection": "default",
  "collectionName": "maintenances",
  "info": {
    "name": "Maintenance"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "html_code": {
      "type": "richtext"
    },
    "apps": {
      "collection": "app",
      "via": "maintenance"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "maintenances",
      "dominant": true
    }
  }
}

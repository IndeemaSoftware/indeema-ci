{
  "connection": "default",
  "collectionName": "ProjectTypes",
  "info": {
    "name": "Project Type"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "value": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "label": {
      "type": "string",
      "required": true
    },
    "apps": {
      "collection": "app",
      "via": "project_type"
    }
  }
}

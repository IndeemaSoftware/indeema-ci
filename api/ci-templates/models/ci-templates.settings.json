{
  "connection": "default",
  "collectionName": "ci_templates",
  "info": {
    "name": "Ci Templates"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "yml_code": {
      "type": "richtext"
    },
    "apps": {
      "collection": "app",
      "via": "ci_template"
    },
    "users": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "ci_templates",
      "dominant": true
    }
  }
}
